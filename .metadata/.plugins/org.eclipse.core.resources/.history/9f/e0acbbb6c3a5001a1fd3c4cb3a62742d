<!-- =======================api -Sandro-=================================== -->
<project name="1.1 api" default="Crear api.jar" >
    <description> Genera el jar del api </description>
	<!-- variables -->
	<property name="base_api" location="D:\eclipseA\workspace\api\bin"/>
	<property name="destino_api" location="D:\eclipseA\workspace\orquestador\api\api.jar"/>
	<!--   apiar   -->
    <target name="Crear api.jar" description="crea el api.jar">
    	 <jar destfile="${destino_api}"  basedir= "${base_api}"/>    	 
    </target>
</project>
